# ===========================================
# CONFIGURAÇÕES DO SUPABASE (OBRIGATÓRIAS)
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ===========================================
# CONFIGURAÇÕES DA APLICAÇÃO
# ===========================================
NEXT_PUBLIC_APP_URL=https://whatsapp-profissional.vercel.app
NEXT_PUBLIC_APP_VERSION=${VERCEL_GIT_COMMIT_SHA}
NEXT_PUBLIC_APP_ENV=production

# ===========================================
# CONFIGURAÇÕES DO VERCEL
# ===========================================
VERCEL_URL=whatsapp-profissional.vercel.app
VERCEL_ENV=production
VERCEL_REGION=

# ===========================================
# CONFIGURAÇÕES DE TIMEZONE
# ===========================================
TZ=America/Sao_Paulo

# ===========================================
# CONFIGURAÇÕES DO Z-API
# ===========================================
Z_API_URL=https://api.z-api.io
Z_API_TOKEN=your_z_api_token

# ===========================================
# CONFIGURAÇÕES DO REDIS (OPCIONAL)
# ===========================================
# REDIS_URL=your_redis_url
# UPSTASH_REDIS_REST_URL=your_upstash_redis_url

# ===========================================
# CONFIGURAÇÕES DO SENTRY (MONITORAMENTO)
# ===========================================
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# ===========================================
# CONFIGURAÇÕES DE LOG
# ===========================================
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# ===========================================
# CONFIGURAÇÕES DO IGNITER.JS
# ===========================================
NEXT_PUBLIC_IGNITER_API_URL=https://whatsapp-profissional.vercel.app
NEXT_PUBLIC_IGNITER_API_BASE_PATH=/api/v1

# ===========================================
# CONFIGURAÇÕES DE EMAIL (OPCIONAL)
# ===========================================
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password