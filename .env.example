# Configurações do Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Configurações da aplicação
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app
NODE_ENV=production

# Configurações do Z-API (opcional - pode ser configurado via interface)
Z_API_BASE_URL=https://api.z-api.io/instances

# Configurações de timezone
TZ=America/Sao_Paulo

# Configurações do Vercel
VERCEL_URL=your-app.vercel.app
VERCEL_ENV=production

# Configurações de Redis (se usando)
REDIS_URL=your_redis_url

# Configurações de email (se usando)
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password

# Configurações de Sentry (opcional)
SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token